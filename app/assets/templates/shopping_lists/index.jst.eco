<form id="new-list-form">
	<input id="new-list" type="text" />
	<button id="add-list">Create New Shopping List</button>
</form>
<ul id="shopping-list-ul">
  <% for item in @shopping_lists.models: %>
    <li id="<%= item.get('id') %>">
      <a href="#/shopping_lists/<%=item.get('id')%>"<span class="name"><%= item.get("name") %></span> <span class="delete-list">X</span></a>
    </li>
  <% end %>
</ul>