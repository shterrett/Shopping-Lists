<section id="login">
  <header>
    <h1>Shopping Lists</h1>
    <h2>Login</h2>
  </header>

  <%= form_for :session, url: sessions_path do |f| %>
    <label>
      <strong>Email Address</strong>
      <%= f.email_field :email %>
    </label>
    <label>
      <strong>Password</strong>
      <%= f.password_field :password %>
    </label>
    
    <%= f.submit "Login"%>
  
  <% end %>
  
</section>